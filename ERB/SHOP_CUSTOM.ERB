	PRINTBUTTON "[キャンセル]",-1
	INPUT
	SIF RESULT < 0
		RESTART
	EXP:ARG:LOCAL = MIN(RESULT,50)
ELSEIF LOCAL:100 == 1006
	
ENDIF
