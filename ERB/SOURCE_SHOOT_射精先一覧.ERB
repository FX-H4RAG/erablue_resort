{
SIF MODE_確認_完全一致("Ｖ挿入系", PLAYER, ARG) == -1
	&& MODE_確認_完全一致("ハーヴィンオナホＶ", PLAYER, ARG) == -1
	&& MODE_確認_完全一致("挿入歩きＶ", PLAYER, ARG) == -1
