;-------------------------------------------------------------------------------------------
;SHOOT_3:乳内
;-------------------------------------------------------------------------------------------
@SHOOT_NAME_3
TSTR:コマンド名受渡 = 乳内射精
SIF TALENT:TARGET:バストサイズ < 1
	TSTR:コマンド名受渡 = 胸に射精
RETURN 1

@SHOOT_PALAM_3(ARG)

CALL SOURCE_CALC_快Ｂ(ARG, PLAYER, 10)
CALL SOURCE_CALC_接触(ARG, PLAYER, 350)

EXP:Ｂ経験 += 1
LOCALS = Ｂ経験
PRINTFORM %LOCALS,15,LEFT%
LOCALS = +1
PRINTFORML %LOCALS,4,LEFT%(%CALLNAME:(ARG)%)

EXP:精液経験 += TFLAG:フィニッシュフラグ
LOCALS = 精液経験
PRINTFORM %LOCALS,15,LEFT%
LOCALS = +{TFLAG:フィニッシュフラグ}
PRINTFORML %LOCALS,4,LEFT%(%CALLNAME:(ARG)%)

@SHOOT_ABLE_3(ARG)
;選択肢に出る条件
;パイズリ系モードじゃない時はアウト
SIF MODE_確認("パイズリ系", ARG) == 0
	RETURN 0
RETURN 1

@MESSAGE_SHOOT_3(ARG)

PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:(ARG)%の
SELECTCASE TALENT:(ARG):バストサイズ
	CASE IS < 1
		PRINT 胸に
	CASE 1
		PRINT 乳の谷間に
	CASE 2
		PRINT 大きな乳の谷間に
	CASE 3
		PRINT とても豊かに育った乳の谷間に
ENDSELECT
SIF TFLAG:フィニッシュフラグ > 1
	PRINT 大量の
PRINT 精液を吐き出した…

