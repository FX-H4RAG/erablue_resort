;PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:(ARG)%の膣奥で
;以上元部分 以下加筆部分
PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:(ARG)%の
PRINTDATA
	DATA 膣奥
;	DATA 最奥
;	DATA 肉穴の最奥
;教えの最奥がチラつく
	DATA 膣の一番奥
	DATA 最も深い場所
	DATA 一番深い場所
ENDDATA
SELECTCASE RAND:2
	CASE 0
		PRINT に
		IF RAND:2 == 0
			PRINTDATA
				DATA 亀頭を
				DATA ペニスの先端を
			ENDDATA
			PRINTDATA
				DATA べったりと
				DATA ぴったりと
				DATA ぐいぐいと
				DATA ぐりぐりと
				DATA 
			ENDDATA
			PRINT 押し付けながら
		ENDIF
	CASEELSE
		PRINT で
ENDSELECT
IF RFLAG:フィニッシュフラグ > 1
	PRINTDATA
		DATA 大量の
		DATA 濃厚な
		DATA おびただしい量の
	ENDDATA
ENDIF	
PRINTDATA
	DATA 精液
	DATA 子種
;	DATA 種汁
;オラッ！APとBP満タンにしてやる！
	DATA ザーメン
ENDDATA
PRINT を
PRINTDATA
	DATA 吐き出した
	DATA 吐き散らした
	DATA 放った
ENDDATA
PRINT …
PRINTL 
CALL 挿入アニメ表示(ARG, "_射精")
