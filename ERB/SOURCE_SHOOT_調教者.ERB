ELSEIF CUP:ARG:快Ｂ + PALAM:ARG:快Ｂ - CDOWN:ARG:快Ｂ >= 絶頂閾値
	B = 1
	CDOWN:ARG:快Ｂ += (絶頂閾値 - 1000)
	NOWEX:ARG:被射精Ｂ絶頂 = 1
	;PRINTFORML 絶頂Ｂ\@ARG == PLAYER?(調教者)#\@
ENDIF
;CDOWN:ARG:快Ｂで下げても絶頂以上なら
;その値-1になるように調整（10000で絶頂なら9999）
SIF CUP:ARG:快Ｂ + PALAM:ARG:快Ｂ - CDOWN:ARG:快Ｂ >= 絶頂閾値
	CDOWN:ARG:快Ｂ = CUP:ARG:快Ｂ + PALAM:ARG:快Ｂ - 絶頂閾値 + 1

;絶頂Ｓ
IF CUP:ARG:快Ｓ + PALAM:ARG:快Ｓ - CDOWN:ARG:快Ｓ >= 絶頂閾値 * 5
	S = 5
	CDOWN:ARG:快Ｓ += (絶頂閾値 * 5 - 1000)
	NOWEX:ARG:被射精Ｓ絶頂 = 5
	;PRINTFORML 究極絶頂Ｓ\@ARG == PLAYER?(調教者)#\@
ELSEIF CUP:ARG:快Ｓ + PALAM:ARG:快Ｓ - CDOWN:ARG:快Ｓ >= 絶頂閾値 * 4
	S = 4
	CDOWN:ARG:快Ｓ += (絶頂閾値 * 4 - 1000)
	NOWEX:ARG:被射精Ｓ絶頂 = 4
	;PRINTFORML 最強絶頂Ｓ\@ARG == PLAYER?(調教者)#\@
ELSEIF CUP:ARG:快Ｓ + PALAM:ARG:快Ｓ - CDOWN:ARG:快Ｓ >= 絶頂閾値 * 3
	S = 3
	CDOWN:ARG:快Ｓ += (絶頂閾値 * 3 - 1000)
	NOWEX:ARG:被射精Ｓ絶頂 = 3
	;PRINTFORML 超強絶頂Ｓ\@ARG == PLAYER?(調教者)#\@
ELSEIF CUP:ARG:快Ｓ + PALAM:ARG:快Ｓ - CDOWN:ARG:快Ｓ >= 絶頂閾値 * 2
	S = 2
	CDOWN:ARG:快Ｓ += (絶頂閾値 * 2 - 1000)
	NOWEX:ARG:被射精Ｓ絶頂 = 2
	;PRINTFORML 強絶頂Ｓ\@ARG == PLAYER?(調教者)#\@
ELSEIF CUP:ARG:快Ｓ + PALAM:ARG:快Ｓ - CDOWN:ARG:快Ｓ >= 絶頂閾値
	S = 1
	CDOWN:ARG:快Ｓ += (絶頂閾値 - 1000)
	NOWEX:ARG:被射精Ｓ絶頂 = 1
	;PRINTFORML 絶頂Ｓ\@ARG == PLAYER?(調教者)#\@
ENDIF
;CDOWN:ARG:快Ｓで下げても絶頂以上なら
;その値-1になるように調整（10000で絶頂なら9999）
SIF CUP:ARG:快Ｓ + PALAM:ARG:快Ｓ - CDOWN:ARG:快Ｓ >= 絶頂閾値
	CDOWN:ARG:快Ｓ = CUP:ARG:快Ｓ + PALAM:ARG:快Ｓ - 絶頂閾値 + 1

;-------------------------------------------------
;絶頂時の追加処理
;-------------------------------------------------
RCVAR:ARG:被射精絶頂の強度 = C + V + A + B + S
IF	ARG == PLAYER
	TFLAG:調教者被射精絶頂強度 = C + V + A + B + S
ENDIF
IF ARG == TARGET
	;快楽刻印の処理
	IF RCVAR:ARG:被射精絶頂の強度 > 3 && MARK:ARG:1 < 3
		RCVAR:ARG:快楽強度 = 3
	;Rが3以上で快楽刻印２に相当
	ELSEIF RCVAR:ARG:被射精絶頂の強度 > 2 && MARK:ARG:1 < 2
		RCVAR:ARG:快楽強度 = 2
	;Rが2以上で快楽刻印１に相当
	ELSEIF RCVAR:ARG:被射精絶頂の強度 > 1 && MARK:ARG:1 < 1
		RCVAR:ARG:快楽強度 = 1
	ELSEIF RCVAR:ARG:被射精絶頂の強度 >= 1
		RCVAR:ARG:快楽強度 = 0
	ENDIF
ENDIF

;同時絶頂による補正
L = 0
SELECTCASE MIN(C, V, A, B, S)
	CASE 5
		L = 5
		C *= 60
		V *= 60
		A *= 60
		B *= 60
		S *= 60
	CASE 4
		L = 5
		C *= 48
		V *= 48
		A *= 48
		B *= 48
		S *= 48
	CASE 3
		L = 5
		C *= 36
		V *= 36
		A *= 36
		B *= 36
		S *= 36
	CASE 2
		L = 5
		C *= 24
		V *= 24
		A *= 24
		B *= 24
		S *= 24
	CASE 1
		L = 5
		C *= 12
		V *= 12
		A *= 12
		B *= 12
		S *= 12
	CASEELSE
		;絶頂状態部位がいくつあるかを検索
		SELECTCASE GROUPMATCH(0, C, V, A, B, S)
			CASE 1
				L = 4
				LOCAL = 8
			CASE 2
				L = 3
				LOCAL = 4
			CASE 3
				L = 2
				LOCAL = 2
			CASE 4
				L = 1
				LOCAL = 1
			ENDSELECT
		C *= LOCAL
		V *= LOCAL
		A *= LOCAL
		B *= LOCAL
		S *= LOCAL
ENDSELECT

IF C
	SOURCE:ARG:露出 += 500 * C
	SOURCE:ARG:屈従 += 200 * C
ENDIF
IF V
	SOURCE:ARG:欲情 += 800 * V
	SOURCE:ARG:恭順 += 500 * V
	SOURCE:ARG:露出 += 700 * V
	SOURCE:ARG:屈従 += 400 * V
ENDIF
IF A
	SOURCE:ARG:欲情 += 500 * A
	SOURCE:ARG:恭順 += 700 * A
	SOURCE:ARG:露出 += 900 * A
	SOURCE:ARG:屈従 += 500 * A
	SOURCE:ARG:逸脱 += 200 * A
ENDIF
IF B
	SOURCE:ARG:露出 += 500 * B
	SOURCE:ARG:屈従 += 200 * B
	SIF TALENT:ARG:性別  == 10
		SOURCE:ARG:逸脱 += 100 * B
ENDIF
IF S
	SOURCE:ARG:露出 += 500 * S
	SOURCE:ARG:屈従 += 200 * S
	SOURCE:ARG:逸脱 += 100 * S
ENDIF

NOWEX:ARG:被射精多重絶頂 = L

;絶頂回数を増やす
EX:ARG:Ｃ絶頂 += NOWEX:ARG:被射精Ｃ絶頂
EX:ARG:Ｖ絶頂 += NOWEX:ARG:被射精Ｖ絶頂
EX:ARG:Ａ絶頂 += NOWEX:ARG:被射精Ａ絶頂
EX:ARG:Ｂ絶頂 += NOWEX:ARG:被射精Ｂ絶頂
EX:ARG:Ｓ絶頂 += NOWEX:ARG:被射精Ｓ絶頂
EX:ARG:多重絶頂 += L

;絶頂経験を増やす
EXP:ARG:絶頂経験 += RCVAR:ARG:被射精絶頂の強度
EXP:ARG:Ｃ絶頂経験 += NOWEX:ARG:被射精Ｃ絶頂
EXP:ARG:Ｖ絶頂経験 += NOWEX:ARG:被射精Ｖ絶頂
EXP:ARG:Ａ絶頂経験 += NOWEX:ARG:被射精Ａ絶頂
EXP:ARG:Ｂ絶頂経験 += NOWEX:ARG:被射精Ｂ絶頂
EXP:ARG:Ｓ絶頂経験 += NOWEX:ARG:被射精Ｓ絶頂
SIF RCVAR:ARG:露出コマンドフラグ
	EXP:ARG:露出絶頂経験 += RCVAR:ARG:被射精絶頂の強度
SIF RCVAR:ARG:苦痛コマンドフラグ
	EXP:ARG:苦痛絶頂経験 += RCVAR:ARG:被射精絶頂の強度
SIF RCVAR:ARG:嗜虐コマンドフラグ
	EXP:ARG:嗜虐絶頂経験 += RCVAR:ARG:被射精絶頂の強度
SIF RCVAR:ARG:奉仕コマンドフラグ
	EXP:ARG:奉仕絶頂経験 += RCVAR:ARG:被射精絶頂の強度
SIF RCVAR:ARG:道具コマンドフラグ
	EXP:ARG:道具絶頂経験 += RCVAR:ARG:被射精絶頂の強度
SIF RCVAR:ARG:触手コマンドフラグ
	EXP:ARG:触手絶頂経験 += RCVAR:ARG:被射精絶頂の強度
IF RCVAR:ARG:精飲フラグ
	EXP:ARG:精飲絶頂経験 += RCVAR:ARG:被射精絶頂の強度
	EXP:ARG:精飲快楽経験 += RCVAR:ARG:被射精絶頂の強度 * 10
ENDIF
IF RCVAR:ARG:膣内射精フラグ
	EXP:ARG:膣内射精絶頂経験 += RCVAR:ARG:被射精絶頂の強度
ENDIF
IF RCVAR:ARG:腸内射精フラグ
	EXP:ARG:腸内射精絶頂経験 += RCVAR:ARG:被射精絶頂の強度
ENDIF

;絶頂による欲望ＬＶアップ
IF ARG == TARGET
	;自制心があると１下がる
	SIF TALENT:ARG:自制心
		L -= 1
	IF ABL:ARG:欲望 < L
		ABL:ARG:欲望 = L
		PRINT そして欲望がLV
		PRINTV L
		PRINTL になった
	ENDIF
ENDIF

;-------------------------------------------------
;絶頂時の追加処理
;-------------------------------------------------
;射精
IF TALENT:ARG:性別 & 2
	NOWEX:ARG:射精 = RCVAR:ARG:被射精絶頂の強度
ENDIF

;ZPの取得
IF RCVAR:ARG:被射精絶頂の強度 > 0
	CALL 絶頂体力減少処理(ARG, NOWEX:ARG:被射精Ｃ絶頂, NOWEX:ARG:被射精Ｖ絶頂, NOWEX:ARG:被射精Ａ絶頂, NOWEX:ARG:被射精Ｂ絶頂, NOWEX:ARG:被射精Ｓ絶頂)
	CALL ZP_取得処理(ARG, RCVAR:ARG:被射精絶頂の強度)
	CFLAG:ARG:体力成長値 += RCVAR:ARG:被射精絶頂の強度 * 5
ENDIF


;連れ込み状態時、満足したか
;ついでに中毒欲求不満の判定も行う
CALL 中毒連れ込み判定_精飲(ARG)
IF RESULT
	;精飲時は必ず絶頂するはずなのでフラグだけ見る
	IF RCVAR:ARG:精飲フラグ
		CFLAG:ARG:中毒欲求不満フラグ = -1
		SIF 連れ込みパターン == "精飲"
			RCVAR:ARG:連れ込み満足フラグ = 1
	ENDIF
ENDIF
CALL 中毒連れ込み判定_膣内射精(ARG)
IF RESULT
	;膣内射精時は必ず絶頂するはずなのでフラグだけ見る
	IF RCVAR:ARG:膣内射精フラグ
		CFLAG:ARG:中毒欲求不満フラグ = -1
		SIF 連れ込みパターン == "膣内射精"
			RCVAR:ARG:連れ込み満足フラグ = 1
	ENDIF
ENDIF
CALL 中毒連れ込み判定_腸内射精(ARG)
IF RESULT
	;腸内射精時は必ず絶頂するはずなのでフラグだけ見る
	IF RCVAR:ARG:腸内射精フラグ
		CFLAG:ARG:中毒欲求不満フラグ = -1
		SIF 連れ込みパターン == "腸内射精"
			RCVAR:ARG:連れ込み満足フラグ = 1
	ENDIF
ENDIF

RETURN 1



