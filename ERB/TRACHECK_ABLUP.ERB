@SOURCE_ABLUP(ARG)

IF ABL:ARG:指 < 6
	LOCAL = EXP:ARG:手淫経験
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:器用な指 * 2 + TALENT:ARG:習得速度
	LOCAL:2 = ABL:ARG:指
	WHILE LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:指 + 3) / 2 && EXP_UP(24,ARG)
		ABL:ARG:指 ++
	WEND
	IF ABL:ARG:指 - LOCAL:2 > 0
		PRINTFORMW %CALLNAME:ARG%の指が器用になった
		IF ABL:ARG:指 >= 5
			PRINTFORMW %CALLNAME:ARG%は[器用な指]を得た
			TALENT:ARG:器用な指 = 1
		ENDIF
	ENDIF
ENDIF
