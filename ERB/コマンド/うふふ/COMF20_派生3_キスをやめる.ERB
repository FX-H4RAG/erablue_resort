
@COMNAME_S20_3
#FUNCTIONS
SELECTCASE COMMODE()
CASE 1 TO 2
	TSTR:コマンド名受渡 = キスをやめる

CASE 3
	TSTR:コマンド名受渡 = キスをやめさせる
ENDSELECT
;複数モード_ダミーをリセット
複数モード_ダミー = 0
;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_S_20_3
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("うふふ_被挿入中_セクハラ")

@COM_TOOLTIP_S20_3
LOCALS = <br>■キスをやめる<br>
LOCALS += "キス系モード中、キスをやめるコマンド。<br>キス系モードを削除。"
LOCALS += "<br>COMタイプ：うふふor被挿入中orセクハラ<br>取得ソース：なし"
LOCALS += "<br>固有の取得経験：なし<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM_S20_3
;-------------------------------------------------
;モードの設定
;-------------------------------------------------
CALL MODE_DELETE("キス系")

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE_S20_3
;キスをやめる実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE_S("20_3")
	RETURN RESULT
;キス系モードじゃない時はアウト
SIF MODE_確認("キス系", TARGET) == 0
	RETURN 0
;助手も同じ体位モードでいるなら同時モードじゃないとダメ
SIF MODE_確認("キス系", 助手は誰だ()) && COMMODE() != 2
	RETURN 0
;助手が同じ体位モードじゃないなら同時モードではダメ
SIF !MODE_確認("キス系", 助手は誰だ()) && COMMODE() == 2
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@MESSAGE_COM_S20_3
PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%から唇を離した…


