@COM62
IF FLAG:モード管理 == 2
	IF RFLAG:コマンド結果受渡し変数 == 12
		TRYCALLFORM COM62_12
		RETURN 0
	ELSE
		LOCAL:10 = TARGET
		FOR LOCAL, 0, 同時モード_選択数
			SIF 同時モード_選択キャラ:LOCAL < 0
				BREAK
			RESULT = 0
			TARGET = 同時モード_選択キャラ:LOCAL
			TRYCALLFORM COM_ABLE62_12
			IF RESULT == 0
				TARGET = LOCAL:10
				RETURN -1
			ENDIF
			RESULT = 1
			TRYCALLFORM CAN_Ａ体位12(1)
			IF !RESULT
				TRYCALLFORM CAN_Ａ体位12(0)
				RETURN -1
			ENDIF
		NEXT
		TARGET = LOCAL:10

		TRYCALLFORM COM62_12
		;メッセージ用に体位の種類を保存
		RFLAG:コマンド結果受渡し変数 = 12
		RETURN 0
	ENDIF
	RETURN 0
ELSE
	$体位_LOOP
	DRAWLINE
	PRINTL ■体位を選んでください
	PRINTL
	FOR LOCAL, 0, 20
		TRYCCALLFORM COM_ABLE62_{LOCAL}
			IF RESULT
				SELECTCASE LOCAL
					CASE 0
						PRINTBUTTONLC "[0]Ａ正常位", 0
					CASE 1
						PRINTBUTTONLC "[1]Ａ後背位", 1
					CASE 2
						PRINTBUTTONLC "[2]Ａ寝バック", 2
					CASE 3
						PRINTBUTTONLC "[3]Ａ対面座位", 3
					CASE 4
						PRINTBUTTONLC "[4]Ａ背面座位", 4
					CASE 5
						PRINTBUTTONLC "[5]Ａ騎乗位", 5
					CASE 6
						PRINTBUTTONLC "[6]Ａ側位", 6
					CASE 7
						PRINTBUTTONLC "[7]Ａ屈曲位", 7
					CASE 8
						PRINTBUTTONLC "[8]Ａ駅弁", 8
					CASE 9
						PRINTBUTTONLC "[9]Ａ背面駅弁", 9
					CASE 10
						PRINTBUTTONLC "[10]Ａ対面立位", 10
					CASE 11
						PRINTBUTTONLC "[11]Ａ立ちバック", 11
				ENDSELECT
			ENDIF
		CATCH
			CONTINUE
		ENDCATCH
	NEXT

	PRINTL
	PRINTL
	PRINTBUTTONLC "[999]戻る", 999

	$INPUT_LOOP
	INPUT

	LOCAL = RESULT
	SELECTCASE LOCAL
		CASE 0 TO 11
			TRYCALLFORM COM_ABLE62_{LOCAL}
			IF RESULT
				IF TARGET == MASTER
					TARGET = PLAYER
					PLAYER = MASTER
					RFLAG:指示モード対象あなたフラグ = 1
					TRYCALLFORM CAN_Ａ体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_Ａ体位{LOCAL}(0)
						GOTO 体位_LOOP
					ENDIF
					RFLAG:指示モード対象あなたフラグ = 0
					PLAYER = TARGET
					TARGET = MASTER
				ELSE
					TRYCALLFORM CAN_Ａ体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_Ａ体位{LOCAL}(0)
						GOTO 体位_LOOP
					ENDIF
				ENDIF
				TRYCALLFORM COM62_{LOCAL}
				;メッセージ用に体位の種類を保存
				RFLAG:コマンド結果受渡し変数 = LOCAL
				RETURN 1
			ENDIF
			GOTO INPUT_LOOP
		CASE 999
			RETURN -1
		CASEELSE
			GOTO INPUT_LOOP
	ENDSELECT
ENDIF

