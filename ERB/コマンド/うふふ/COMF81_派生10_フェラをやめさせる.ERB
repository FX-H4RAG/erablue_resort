
@COMNAME_S81_10
#FUNCTIONS
TSTR:コマンド名受渡 = フェラをやめさせる

SIF MODE_確認("パイズリフェラ", TARGET)
	TSTR:コマンド名受渡 = パイズリフェラをやめさせる

;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_S_81_10
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("うふふ")
同時モード許可 = 1
助手モード許可 = 1

@COM_TOOLTIP_S81_10
LOCALS = <br>■フェラをやめさせる<br>
LOCALS += "フェラチオモードを終了するコマンド。<br>パイズリフェラモード時、[パイズリフェラをやめさせる]に変化。"
LOCALS += "<br>COMタイプ：うふふ<br>取得ソース：なし"
LOCALS += "<br>固有の取得経験：なし<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM_S81_10
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------

;-------------------------------------------------
;モードの設定
;-------------------------------------------------
CALL MODE_DELETE("フェラチオ")
CALL MODE_DELETE("パイズリフェラ")

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE_S81_10
;フェラチオをやめる実行判定
SIF !TFLAG:ゲームフェイズ管理
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE_S("81_10")
	RETURN RESULT
;フェラチオ系モードじゃない時はアウト
SIF MODE_確認("フェラチオ系", TARGET) == 0
	RETURN 0
;助手も同じ体位モードでいるなら同時モードじゃないとダメ
SIF MODE_確認("フェラチオ", 助手は誰だ()) && COMMODE() != 2
	RETURN 0
;助手が同じ体位モードじゃないなら同時モードではダメ
SIF !MODE_確認("フェラチオ", 助手は誰だ()) && COMMODE() == 2
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@MESSAGE_COM_S81_10
PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%を止め、口から陰茎を引き抜いた


