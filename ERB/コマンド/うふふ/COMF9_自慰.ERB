;-------------------------------------------------
;自慰
;愛撫系コマンド、レベル3
;-------------------------------------------------
@COMNAME9
#FUNCTIONS
SELECTCASE COMMODE()
CASE 1
	TSTR:コマンド名受渡 = 自慰
CASE 2
	TSTR:コマンド名受渡 = 同時に自慰させる
ENDSELECT

;複数モード_ダミーをリセット
複数モード_ダミー = 0

;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_9
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("うふふ")
弱点コマンドカテゴリ = U
同時モード許可 = 1

@COM_TOOLTIP_9
LOCALS = <br>■自慰<br>
LOCALS += "相手にその場で自慰をさせるコマンド。"
LOCALS += "<br>COMタイプ：うふふ<br>取得ソース：快Ｃ(一定条件時)・快Ｖ(一定条件時)・快Ｂ(一定条件時)・快Ａ(一定条件時)・苦痛(一定条件時)・露出"
LOCALS += "<br>固有の取得経験：自慰経験(TARGET)・調教自慰経験(TARGET)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM9
#DIM 快定数
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 5
DOWNBASE:気力 += 50

;快計算
快定数 = 80

CALL SOURCE_CALC_露出(TARGET, PLAYER, 400)
;二穴バイブ
IF MODE_確認("バイブ", TARGET) && MODE_確認("アナルバイブ", TARGET)
	RFLAG:コマンド結果受渡し変数 = 13
	CALL SOURCE_CALC_快Ｖ(TARGET, TARGET, 快定数)
	CALL SOURCE_CALC_快Ａ(TARGET, TARGET, 快定数)
	CALL SOURCE_CALC_露出(TARGET, PLAYER, 80)
	;苦痛過剰になりそうなので単体より苦痛軽減
	CALL SOURCE_CALC_苦痛(TARGET, PLAYER, 60 * PAIN_CHECK_V(TARGET) / 1000)
	CALL SOURCE_CALC_苦痛(TARGET, PLAYER, 80 * PAIN_CHECK_A(TARGET) / 1000)
;バイブ
ELSEIF MODE_確認("バイブ", TARGET)
	RFLAG:コマンド結果受渡し変数 = 11
	CALL SOURCE_CALC_快Ｖ(TARGET, TARGET, 快定数)
	CALL SOURCE_CALC_露出(TARGET, PLAYER, 50)
	CALL SOURCE_CALC_苦痛(TARGET, PLAYER, 80 * PAIN_CHECK_V(TARGET) / 1000)
;アナルバイブ
ELSEIF MODE_確認("アナルバイブ", TARGET)
	RFLAG:コマンド結果受渡し変数 = 12
	CALL SOURCE_CALC_快Ａ(TARGET, TARGET, 快定数)
	CALL SOURCE_CALC_露出(TARGET, PLAYER, 50)
	CALL SOURCE_CALC_苦痛(TARGET, PLAYER, 100 * PAIN_CHECK_A(TARGET) / 1000)
; ;シャワー
; ELSEIF TEQUIP:32
; 	CALL SOURCE_CALC_快Ｃ(TARGET, TARGET, 快定数)
; 	CALL SOURCE_CALC_露出(TARGET, PLAYER, 100)
; ;ビデオ
; ELSEIF TEQUIP:28
; 	CALL SOURCE_CALC_液体(TARGET, PLAYER, 50)
; 	CALL SOURCE_CALC_露出(TARGET, PLAYER, 300)
ELSE
	RFLAG:コマンド結果受渡し変数 = RANDOM_PART(TARGET)
	SELECTCASE RFLAG:コマンド結果受渡し変数
		CASE 0
			CALL SOURCE_CALC_快Ｃ(TARGET, TARGET, 快定数)
		CASE 1
			CALL SOURCE_CALC_快Ｖ(TARGET, TARGET, 快定数)
		CASE 2
			CALL SOURCE_CALC_快Ａ(TARGET, TARGET, 快定数)
		CASE 3
			CALL SOURCE_CALC_快Ｂ(TARGET, TARGET, 快定数)
	ENDSELECT
ENDIF
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ビデオ撮影時は経験にプラス
IF TEQUIP:ビデオ撮影
	EXP:自慰経験 += 2
	EXP:調教自慰経験 += 2
ELSE
	EXP:自慰経験 += 1
	EXP:調教自慰経験 += 1
ENDIF
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:同性愛経験 += 4
	EXP:PLAYER:同性愛経験 += 4
ENDIF
RETURN 1


@CAN_COM9(ARG)
SKIPDISP ARG
SIF TEQUIP:ビデオ撮影
	PRINT 公開
SIF TEQUIP:シャワー
	PRINT シャワー
IF MODE_確認("バイブ", TARGET) && MODE_確認("アナルバイブ", TARGET)
	PRINT 二穴バイブ
ELSEIF MODE_確認("バイブ", TARGET)
	PRINT バイブ
ELSEIF MODE_確認("アナルバイブ", TARGET)
	PRINT アナルバイブ
ENDIF

IF TEQUIP:ビデオ撮影 || TEQUIP:シャワー || MODE_確認("バイブ", TARGET) || MODE_確認("アナルバイブ", TARGET)
	PRINTL オナニー
ELSE
	PRINTL 自慰
ENDIF
;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
VARSET A
;すべての命令に共通の要素を考慮

CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 3
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
	S = 1
ENDIF
;ABL:露出癖
IF ABL:露出癖
	SIF S
		PRINT  + 
	A += ABL:露出癖 * 4
	PRINT 露出癖
	PRINTV 'LV,ABL:露出癖,'(,ABL:露出癖 * 4,')
	S = 1
ENDIF
;ABL:自慰中毒
IF ABL:自慰中毒
	SIF S
		PRINT  + 
	A += ABL:自慰中毒 * 3
	PRINT 自慰中毒
	PRINTV 'LV,ABL:自慰中毒,'(,ABL:自慰中毒 * 3,')
	S = 1
ENDIF

IF MARK:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:快楽刻印 * 3
	PRINT 快楽刻印
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 3,')
	S = 1
ENDIF

;自制心
IF TALENT:自制心
	PRINT  - 
	A -= 5
	PRINT 自制心
	PRINTV '(,5,')
	S = 1
ENDIF
;恥じらい
IF TALENT:羞恥心 > 0
	PRINT  - 
	A -= 5
	PRINT 恥じらい
	PRINTV '(,5,')
	S = 1
ENDIF
;恥薄い
IF TALENT:羞恥心 < 0
	SIF S
		PRINT  + 
	A += 2
	PRINT 恥薄い
	PRINTV '(,2,')
	S = 1
ENDIF
;自慰しやすい
IF TALENT:自慰しやすい
	SIF S
		PRINT  + 
	A += 5
	PRINT 自慰しやすい
	PRINTV '(,5,')
	S = 1
ENDIF
;快感に素直
IF TALENT:快感応答 > 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 快感に素直
	PRINTV '(,5,')
	S = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感応答 < 0 && !TALENT:淫乱
	PRINT  - 
	A -= 5
	PRINT 快感の否定
	PRINTV '(,5,')
	S = 1
ENDIF

;媚薬
IF TEQUIP:媚薬
	SIF S
		PRINT  + 
	A += 8
	PRINT 媚薬
	PRINTV '(,8,')
	S = 1
ENDIF

;合計を表示(33以上で実行)
PRINT  = 
PRINTV A

;難易度上昇
;公開で+10、バイブで+5、アナルバイブで+5、シャワーで＋3
V = 33
SIF TEQUIP:ビデオ撮影
	V += 10
SIF TEQUIP:シャワー
	V += 3
SIF MODE_確認("バイブ", TARGET)
	V += 5
SIF MODE_確認("アナルバイブ", TARGET)
	V += 5

SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT
SKIPDISP 0
;実行できない
SIF A < V
	RETURN 0
RETURN 1


;-------------------------------------------------
;実行判定
;-------------------------------------------------

@COM_ABLE9
;自慰実行判定
SIF !TFLAG:ゲームフェイズ管理
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(9)
	RETURN RESULT
;鎧があると不可
SIF TEQUIP:上半身鎧あり
	RETURN 0
SIF TEQUIP:下腹部前鎧あり
	RETURN 0
;縄使用中は不可
SIF TEQUIP:縄
	RETURN 0
RETURN 1

