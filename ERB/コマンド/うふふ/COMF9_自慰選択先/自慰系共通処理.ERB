SIF TALENT:PLAYER:器用な指
	CALL SOURCE_CALC_快Ｃ(PLAYER, -1, 100)
CALL SOURCE_CALC_露出(PLAYER, TARGET, 150)

SELECTCASE EXP:PLAYER:調教自慰経験
    CASE IS > 50
        CALL SOURCE_CALC_歓楽(PLAYER, TARGET, 100)
