@MODE_確認(ARGS, ARG)
#FUNCTION
;ARGSのモードがあるかどうかを見る
FOR LOCAL, 0, 6
	IF 体位モード格納:ARG:LOCAL == ARGS
		RETURNF 1
	ENDIF
	IF ARGS == "キス系"
		SIF MODE系統確認(体位モード格納:ARG:LOCAL, "キス系")
			RETURNF 1
	ENDIF
	IF ARGS == "Ｖ挿入系"
		SIF MODE系統確認(体位モード格納:ARG:LOCAL, "Ｖ挿入系")
			RETURNF 1
	ENDIF
	IF ARGS == "Ａ挿入系"
		SIF MODE系統確認(体位モード格納:ARG:LOCAL, "Ａ挿入系")
			RETURNF 1
	ENDIF
	IF ARGS == "フェラチオ系"
		SIF MODE系統確認(体位モード格納:ARG:LOCAL, "フェラチオ系")
			RETURNF 1
	ENDIF
	IF ARGS == "パイズリ系"
		SIF MODE系統確認(体位モード格納:ARG:LOCAL, "パイズリ系")
			RETURNF 1
	ENDIF
	IF ARGS == "手コキ系"
		SIF MODE系統確認(体位モード格納:ARG:LOCAL, "手コキ系")
			RETURNF 1
	ENDIF
	IF ARGS == "乳首吸い系"
		SIF MODE系統確認(体位モード格納:ARG:LOCAL, "乳首吸い系")
			RETURNF 1
	ENDIF
	IF ARGS == "逆アナル系"
		SIF MODE系統確認(体位モード格納:ARG:LOCAL, "逆アナル系")
			RETURNF 1
	ENDIF
NEXT

@MODE系統確認(モード名, 系統名)
#FUNCTION
#DIMS モード名
#DIMS 系統名
IF 系統名 == "キス系"
	SIF GROUPMATCH(モード名, "キス", "ディープキス")
		RETURNF 1
ENDIF
IF 系統名 == "Ｖ挿入系"
	SIF GROUPMATCH(モード名, "正常位", "後背位", "寝バック", "対面座位", "背面座位", "騎乗位", "側位", "屈曲位", "駅弁", "逆駅弁", "対面立位", "立ちバック")
		RETURNF 1
ENDIF
IF 系統名 == "Ａ挿入系"
	SIF GROUPMATCH(モード名, "Ａ正常位", "Ａ後背位", "Ａ寝バック", "Ａ対面座位", "Ａ背面座位", "Ａ騎乗位", "Ａ側位", "Ａ屈曲位", "Ａ駅弁", "Ａ逆駅弁", "Ａ対面立位", "Ａ立ちバック")
		RETURNF 1
ENDIF
IF 系統名 == "フェラチオ系"
	SIF GROUPMATCH(モード名, "フェラチオ", "パイズリフェラ")
		RETURNF 1
ENDIF
IF 系統名 == "パイズリ系"
	SIF GROUPMATCH(モード名, "パイズリ", "パイズリフェラ")
		RETURNF 1
ENDIF
IF 系統名 == "手コキ系"
	SIF GROUPMATCH(モード名, "手コキ", "授乳手コキ")
		RETURNF 1
ENDIF
IF 系統名 == "乳首吸い系"
	SIF GROUPMATCH(モード名, "乳首吸い", "授乳手コキ", "助手乳首吸い")
		RETURNF 1
ENDIF
IF 系統名 == "逆アナル系"
	SIF GROUPMATCH(モード名, "逆Ａ正常位", "逆Ａ後背位", "逆Ａ寝バック", "逆Ａ対面座位", "逆Ａ背面座位", "逆Ａ騎乗位", "逆Ａ側位", "逆Ａ屈曲位", "逆Ａ駅弁", "逆Ａ逆駅弁", "逆Ａ対面立位", "逆Ａ立ちバック")
		RETURNF 1
ENDIF



@SHOW_MODE(ARG)
LOCAL:1 = 0

FOR LOCAL, 0, 6
	IF 体位モード格納:ARG:LOCAL != ""
		LOCAL:1 += 1
		IF LOCAL:1 == 1
			SIF ARG == TARGET:1
				DRAWLINE
			PRINTFORM %CALLNAME:ARG%：
		ELSE
			FOR LOCAL:2, 0, STRLENS(CALLNAME:ARG) + 2
				PRINT  
			NEXT
		ENDIF
		SETCOLOR カラーパレット("えっちな色")
		;助手乳首吸いは表記を修正する
		IF 体位モード格納:ARG:LOCAL == "助手乳首吸い" || 体位モード格納:ARG:LOCAL == "乳首吸い"
			PRINTFORM [乳首吸い継続中
		ELSE
			PRINTFORM [%体位モード格納:ARG:LOCAL%継続中
		ENDIF
		;3Pならモード主犯も添える
		SIF 助手は誰だ()
			PRINTFORM (%NAME:(モード主犯:ARG:LOCAL)%)
		PRINT_IMG "えっちハート"
		PRINT ]
		RESETCOLOR
		CALL MODE詳細設定処理(ARG, 体位モード格納:ARG:LOCAL)
		PRINTL 
	ENDIF
NEXT

