@MODE_確認(ARGS, ARG)
#FUNCTION
;ARGSのモードがあるかどうかを見る
FOR LOCAL, 0, 5
	IF 体位モード格納:ARG:LOCAL == ARGS
		RETURNF 1
	ENDIF
	IF ARGS == "キス系"
		SIF GROUPMATCH(体位モード格納:ARG:LOCAL, "キス", "ディープキス")
			RETURNF 1
	ENDIF
	IF ARGS == "Ｖ挿入系"
		SIF GROUPMATCH(体位モード格納:ARG:LOCAL, "正常位", "後背位", "寝バック", "対面座位", "背面座位", "騎乗位", "側位", "屈曲位", "駅弁", "逆駅弁", "対面立位", "立ちバック")
			RETURNF 1
	ENDIF
	IF ARGS == "Ａ挿入系"
		SIF GROUPMATCH(体位モード格納:ARG:LOCAL, "Ａ正常位", "Ａ後背位", "Ａ寝バック", "Ａ対面座位", "Ａ背面座位", "Ａ騎乗位", "Ａ側位", "Ａ屈曲位", "Ａ駅弁", "Ａ逆駅弁", "Ａ対面立位", "Ａ立ちバック")
			RETURNF 1
	ENDIF
	IF ARGS == "フェラチオ系"
		SIF GROUPMATCH(体位モード格納:ARG:LOCAL, "フェラチオ", "パイズリフェラ")
			RETURNF 1
	ENDIF
	IF ARGS == "パイズリ系"
		SIF GROUPMATCH(体位モード格納:ARG:LOCAL, "パイズリ", "パイズリフェラ")
			RETURNF 1
	ENDIF
	IF ARGS == "手コキ系"
		SIF GROUPMATCH(体位モード格納:ARG:LOCAL, "手コキ", "授乳手コキ")
			RETURNF 1
	ENDIF
	IF ARGS == "乳首吸い系"
		SIF GROUPMATCH(体位モード格納:ARG:LOCAL, "乳首吸い", "授乳手コキ")
			RETURNF 1
	ENDIF
	IF ARGS == "逆アナル系"
		SIF GROUPMATCH(体位モード格納:ARG:LOCAL, "逆Ａ正常位", "逆Ａ後背位", "逆Ａ寝バック", "逆Ａ対面座位", "逆Ａ背面座位", "逆Ａ騎乗位", "逆Ａ側位", "逆Ａ屈曲位", "逆Ａ駅弁", "逆Ａ逆駅弁", "逆Ａ対面立位", "逆Ａ立ちバック")
			RETURNF 1
	ENDIF
NEXT

@SHOW_MODE(ARG)
LOCAL:1 = 0

FOR LOCAL, 0, 5
	IF 体位モード格納:ARG:LOCAL != ""
		LOCAL:1 += 1
		IF LOCAL:1 == 1
			SIF ARG == TARGET:1
				DRAWLINE
			PRINTFORM %CALLNAME:ARG%：
		ELSE
			FOR LOCAL:2, 0, STRLENS(CALLNAME:ARG) + 2
				PRINT  
			NEXT
		ENDIF
		SETCOLOR カラーパレット("えっちな色")
		PRINTFORM [%体位モード格納:ARG:LOCAL%継続中
		PRINT_IMG "えっちハート"
		PRINT ]
		RESETCOLOR
		CALL MODE詳細設定処理(ARG, 体位モード格納:ARG:LOCAL)
		PRINTL 
	ENDIF
NEXT

@MODE詳細設定処理(キャラ番号, 体位モード)
#DIM キャラ番号
#DIMS 体位モード

;V挿入系
IF GROUPMATCH(体位モード格納:キャラ番号:LOCAL, "正常位", "後背位", "寝バック", "対面座位", "背面座位", "騎乗位", "側位", "屈曲位", "駅弁", "逆駅弁", "対面立位", "立ちバック")
	
ENDIF



