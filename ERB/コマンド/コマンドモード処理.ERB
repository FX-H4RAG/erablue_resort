	CASE 0
		SIF ARGS == "プレイヤー"
			RETURN 1
		SIF ARGS == "ターゲット"
			RETURN 2
	CASE 1
		SIF ARGS == "ターゲット"
			RETURN 2
	CASE 3
		SIF ARGS == "ターゲット"
			RETURN 1
		SIF ARGS == "プレイヤー"
