;挿入で使用するのはペニスと膣で1を返す
SELECTCASE ARG
	CASE 0
		SIF ARGS == "ターゲット"
			RETURN 1
	CASE 2
		;抱えるので手は使用中
		SIF ARGS == "ターゲット"
			RETURN 1
	CASE 3
		;口は届かない
		RETURN 2
	CASE 4
		;胸は届かない
		RETURN 2
	CASE 5
		SIF ARGS == "ターゲット"
			RETURN 2
	CASE 6
		SIF ARGS == "プレイヤー"
			RETURN 1
		SIF ARGS == "ターゲット"
			RETURN 2
ENDSELECT

