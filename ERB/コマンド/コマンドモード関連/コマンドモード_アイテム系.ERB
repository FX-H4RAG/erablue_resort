@MODE詳細設定補正_バイブ(キャラ番号, 補正先種類)
#DIM キャラ番号
#DIMS 補正先種類
VARSET LOCAL

SELECTCASE 補正先種類
	CASE "快Ｖ"
		IF ABL:キャラ番号:Ｖ感覚 >= バイブ強さ:キャラ番号:0
			LOCAL += 10 * バイブ強さ:キャラ番号:0
		ENDIF
		RETURN LOCAL
ENDSELECT

