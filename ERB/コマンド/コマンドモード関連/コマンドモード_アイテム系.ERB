@MODE_持続快楽_アイテム系(キャラ番号)
#DIM キャラ番号
#DIM 主犯
#DIM 快楽発生フラグ
#DIM 詳細設定補正

快楽発生フラグ = 0
IF MODE_確認("アイテム系", キャラ番号)
	RCVAR:キャラ番号:道具コマンドフラグ = 1
ELSE
	RETURN 0
ENDIF

IF MODE_確認("クリキャップ", キャラ番号)
	主犯 = PLAYER
	CALL SOURCE_CALC_快Ｃ(キャラ番号, 主犯, 50)
	EXP:キャラ番号:Ｃ経験 ++
	快楽発生フラグ = 1
ENDIF

IF MODE_確認("オナホール", キャラ番号)
	主犯 = PLAYER
	CALL SOURCE_CALC_快Ｃ(キャラ番号, 主犯, 50)
	EXP:キャラ番号:Ｃ経験 ++
	快楽発生フラグ = 1
ENDIF

IF MODE_確認("バイブ", キャラ番号)
	主犯 = PLAYER
	CALL SOURCE_CALC_快Ｖ(キャラ番号, 主犯, 50)
	EXP:キャラ番号:Ｖ経験 ++
	快楽発生フラグ = 1
ENDIF

IF MODE_確認("アナルバイブ", キャラ番号)
	主犯 = PLAYER
	CALL SOURCE_CALC_快Ａ(キャラ番号, 主犯, 50)
	EXP:キャラ番号:Ａ経験 ++
	快楽発生フラグ = 1
ENDIF

IF MODE_確認("アナルビーズ", キャラ番号)
	主犯 = PLAYER
	CALL SOURCE_CALC_快Ａ(キャラ番号, 主犯, 50)
	EXP:キャラ番号:Ａ経験 ++
	快楽発生フラグ = 1
ENDIF

IF MODE_確認("ニプルキャップ", キャラ番号)
	主犯 = PLAYER
	CALL SOURCE_CALC_快Ｂ(キャラ番号, 主犯, 50)
	EXP:キャラ番号:Ｂ経験 ++
	RCVAR:キャラ番号:乳首開発フラグ = 2
	快楽発生フラグ = 1
ENDIF

IF MODE_確認("搾乳機", キャラ番号)
	主犯 = PLAYER
	CALL SOURCE_CALC_快Ｂ(キャラ番号, 主犯, 50)
	EXP:キャラ番号:Ｂ経験 ++
	RCVAR:キャラ番号:乳首開発フラグ = 2
	快楽発生フラグ = 1
ENDIF

RETURN 快楽発生フラグ

@MODETYPE_クリキャップ(ARG, ARGS)
;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;アイテムのためプレイヤー側は一切場所を専有しない
SELECTCASE ARG
	CASE 1
		SIF ARGS == "ターゲット"
			RETURN 1
ENDSELECT

