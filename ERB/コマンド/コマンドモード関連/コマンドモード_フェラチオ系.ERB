EXP:実行キャラ番号:Ｓ経験 ++
EXP:実行キャラ番号:口淫経験 ++
EXP:対象キャラ番号:Ｃ経験 ++
;奉仕快楽
RCVAR:実行キャラ番号:奉仕コマンドフラグ = 1

RETURN 1


@MODETYPE_フェラチオ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%のペニスを口淫する
	;下の部位専有判定と別にするため9を返す
	RETURN 9
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%の口でペニスを責められている
	;下の部位専有判定と別にするため9を返す
	RETURN 9
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
SELECTCASE 部位番号
	CASE 0
		SIF 処理モード == "ターゲット"
			RETURN 1
		SIF 処理モード == "プレイヤー"
			RETURN 2
	CASE 1
		SIF 処理モード == "プレイヤー"
			RETURN 2
	CASE 3
		SIF 処理モード == "プレイヤー"
			RETURN 1
		SIF 処理モード == "ターゲット"
			RETURN 2
	CASE 4
		;プレイヤーの胸には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 2
	CASE 5
		;プレイヤーの膣には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 2
	CASE 6
		;プレイヤーの尻には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 2
ENDSELECT

@MODETYPE_パイズリフェラ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%のペニスを乳で挟み、更に口淫する
	;下の部位専有判定と別にするため9を返す
	RETURN 9
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%の乳と口でペニスを責められている
	;下の部位専有判定と別にするため9を返す
	RETURN 9
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;キスは両者口のみ専有
SELECTCASE 部位番号
	CASE 0
		SIF 処理モード == "ターゲット"
			RETURN 1
	CASE 1
		SIF 処理モード == "プレイヤー"
			RETURN 2
	CASE 2
		SIF 処理モード == "プレイヤー"
			RETURN 1
	CASE 3
		SIF 処理モード == "プレイヤー"
			RETURN 1
		SIF 処理モード == "ターゲット"
			RETURN 2
	CASE 4
		SIF 処理モード == "プレイヤー"
			RETURN 1
	CASE 5
		;プレイヤーの膣には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 2
	CASE 6
		;プレイヤーの尻には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 2
ENDSELECT


