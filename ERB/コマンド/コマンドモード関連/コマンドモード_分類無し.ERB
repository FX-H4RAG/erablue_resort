ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;キスは両者口のみ専有
SELECTCASE 部位番号
	CASE 0
		RETURN 1
	CASE 1
		RETURN 1
	CASE 3
		RETURN 1
	CASE 4
		RETURN 2
ENDSELECT


