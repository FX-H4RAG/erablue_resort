@MODETYPE_逆Ａ寝バック(ARG, ARGS)
;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;挿入で使用するのはペニスと膣で1を返す
SELECTCASE ARG
	CASE 0
		SIF ARGS == "ターゲット"
			RETURN 1
		SIF ARGS == "プレイヤー"
			RETURN 2
	CASE 1
		;クリは届かない
		SIF ARGS == "プレイヤー"
			RETURN 2
	CASE 3
		;口は届かない
		RETURN 2
	CASE 4
		;胸は届かない
		RETURN 2
	CASE 5
		SIF ARGS == "プレイヤー"
			RETURN 2
		SIF ARGS == "ターゲット"
			RETURN 2
	CASE 6
		SIF ARGS == "プレイヤー"
			RETURN 1
		;ターゲットの尻は届かない
		SIF ARGS == "ターゲット"
			RETURN 2
ENDSELECT

