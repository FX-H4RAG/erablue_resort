@系統リスト_逆アナル系(モード名)
#FUNCTION
#DIMS モード名
IF GROUPMATCH(モード名, "逆Ａ正常位", "逆Ａ後背位", "逆Ａ寝バック", "逆Ａ対面座位", "逆Ａ背面座位", "逆Ａ騎乗位", "逆Ａ側位", "逆Ａ屈曲位", "逆Ａ駅弁", "逆Ａ逆駅弁", "逆Ａ対面立位", "逆Ａ立ちバック")
	モード確認結果 = 1
	RETURNF 1
ENDIF

@MODE_持続快楽_逆アナル系(キャラ番号)
#DIM キャラ番号
#DIM 主犯

IF MODE_確認("逆アナル系", キャラ番号)
	主犯 = MODE_主犯確認("逆アナル系", キャラ番号)
	CALL SOURCE_CALC_快Ａ(主犯, キャラ番号, 100)
	CALL SOURCE_CALC_接触(キャラ番号, 主犯, 100)
	CALL SOURCE_CALC_快Ｃ(キャラ番号, 主犯, 200)
	EXP:キャラ番号:Ｃ経験 ++
	EXP:キャラ番号:Ｃ性交経験 ++
	RETURN 1
ENDIF


@MODETYPE_逆Ａ正常位(ARG, ARGS)
;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;挿入で使用するのはペニスと膣で1を返す
SELECTCASE ARG
	CASE 0
		SIF ARGS == "ターゲット"
			RETURN 1
	CASE 3
		;両方ハーヴィンだと口は届く
		SIF TALENT:PLAYER:種族 == 4 && TALENT:TARGET:種族 == 4
			RETURN 0
		;どっちかがハーヴィンだと口は届かない
		SIF TALENT:PLAYER:種族 == 4 || TALENT:TARGET:種族 == 4
			RETURN 2
	CASE 6
		SIF ARGS == "プレイヤー"
			RETURN 1
		SIF ARGS == "ターゲット"
			RETURN 2
ENDSELECT

