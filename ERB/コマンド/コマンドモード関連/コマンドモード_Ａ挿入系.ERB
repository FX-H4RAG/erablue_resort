@MODE詳細設定補正_Ａ挿入系(キャラ番号, 補正先種類)
#DIM キャラ番号
#DIMS 補正先種類
VARSET LOCAL

SELECTCASE 補正先種類
	CASE "快Ａ"
		IF ABL:キャラ番号:Ａ感覚 >= A挿入強さ:キャラ番号:0
			LOCAL += 10 * A挿入強さ:キャラ番号:0
		ENDIF
		IF PALAM:キャラ番号:潤滑 >= A挿入速さ:キャラ番号:0 + 3
			LOCAL += 10 * A挿入速さ:キャラ番号:0
		ENDIF
		RETURN LOCAL
	CASE "苦痛"
		IF ABL:キャラ番号:Ａ感覚 >= A挿入強さ:キャラ番号:0
			LOCAL += A挿入強さ:キャラ番号:0 * 200 * PAIN_CHECK_A(キャラ番号) / 1000
		ENDIF
		IF PALAM:キャラ番号:潤滑 >= A挿入速さ:キャラ番号:0 + 3
			LOCAL += A挿入速さ:キャラ番号:0 * 200 * PAIN_CHECK_A(キャラ番号) / 1000
		ENDIF
		RETURN LOCAL
ENDSELECT

@MODETYPE_Ａ正常位(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = 上から%CALLNAME:ターゲット番号%に覆いかぶさり、アナルにペニスを突き入れる
	;下の部位専有判定と別にするため9を返す
	RETURN 9
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = 仰向けになって、アナルで%CALLNAME:プレイヤー番号%のペニスを咥え込んだ
	;下の部位専有判定と別にするため9を返す
	RETURN 9
ENDIF

;以下部位の専有判定
;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;挿入で使用するのはペニスと膣で1を返す
SELECTCASE 部位番号
	CASE 0
		SIF 処理モード == "プレイヤー"
			RETURN 1
	CASE 3
		;両方ハーヴィンだと口は届く
		SIF TALENT:プレイヤー番号:種族 == 4 && TALENT:ターゲット番号:種族 == 4
			RETURN 0
		;どっちかがハーヴィンだと口は届かない
		SIF TALENT:プレイヤー番号:種族 == 4 || TALENT:ターゲット番号:種族 == 4
			RETURN 2
	CASE 6
		SIF 処理モード == "ターゲット"
			RETURN 1
		SIF 処理モード == "プレイヤー"
			RETURN 2
ENDSELECT

