@系統リスト_Ａ挿入系(モード名)
#FUNCTION
#DIMS モード名
IF GROUPMATCH(モード名, "Ａ正常位", "Ａ後背位", "Ａ寝バック", "Ａ対面座位", "Ａ背面座位", "Ａ騎乗位", "Ａ側位", "Ａ屈曲位", "Ａ駅弁", "Ａ逆駅弁", "Ａ対面立位", "Ａ立ちバック", "Ａ同時挿入")
	RETURNF 1
ENDIF

RETURNF 0

@MODE_持続快楽_Ａ挿入系(キャラ番号)
#DIM キャラ番号
#DIM 主犯
#DIM 詳細設定補正

IF MODE_確認("Ａ挿入系", キャラ番号)
	主犯 = MODE_主犯確認("Ａ挿入系", キャラ番号)
	CALL MODE詳細設定補正_Ａ挿入系(キャラ番号, "快Ａ")
	詳細設定補正 = 100 + RESULT
	CALL SOURCE_CALC_快Ａ(キャラ番号, 主犯, 詳細設定補正)
	CALL SOURCE_CALC_接触(キャラ番号, 主犯, 100)
	CALL MODE詳細設定補正_Ａ挿入系(キャラ番号, "苦痛")
	詳細設定補正 = RESULT
	CALL SOURCE_CALC_苦痛(キャラ番号, 主犯, 詳細設定補正)
	CALL SOURCE_CALC_快Ｃ(主犯, キャラ番号, 200)
	EXP:キャラ番号:Ａ性交経験 ++
	EXP:キャラ番号:Ａ経験 ++
	RETURN 1
ENDIF

@MODE詳細設定補正_Ａ挿入系(キャラ番号, 補正先種類)
#DIM キャラ番号
#DIMS 補正先種類
VARSET LOCAL

SELECTCASE 補正先種類
	CASE "快Ａ"
		IF ABL:キャラ番号:Ａ感覚 >= A挿入強さ:キャラ番号:0
			LOCAL += 10 * A挿入強さ:キャラ番号:0
		ENDIF
		IF PALAM:キャラ番号:潤滑 >= A挿入速さ:キャラ番号:0 + 3
			LOCAL += 10 * A挿入速さ:キャラ番号:0
		ENDIF
		RETURN LOCAL
	CASE "苦痛"
		IF ABL:キャラ番号:Ａ感覚 >= A挿入強さ:キャラ番号:0
			LOCAL += A挿入強さ:キャラ番号:0 * 200 * PAIN_CHECK_V(TARGET) / 1000
		ENDIF
		IF PALAM:キャラ番号:潤滑 >= A挿入速さ:キャラ番号:0 + 3
			LOCAL += A挿入速さ:キャラ番号:0 * 200 * PAIN_CHECK_V(TARGET) / 1000
		ENDIF
		RETURN LOCAL
ENDSELECT

@MODETYPE_Ａ正常位(ARG, ARGS)
;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;挿入で使用するのはペニスと膣で1を返す
SELECTCASE ARG
	CASE 0
		SIF ARGS == "プレイヤー"
			RETURN 1
	CASE 3
		;両方ハーヴィンだと口は届く
		SIF TALENT:PLAYER:種族 == 4 && TALENT:TARGET:種族 == 4
			RETURN 0
		;どっちかがハーヴィンだと口は届かない
		SIF TALENT:PLAYER:種族 == 4 || TALENT:TARGET:種族 == 4
			RETURN 2
	CASE 6
		SIF ARGS == "ターゲット"
			RETURN 1
		SIF ARGS == "プレイヤー"
			RETURN 2
ENDSELECT

