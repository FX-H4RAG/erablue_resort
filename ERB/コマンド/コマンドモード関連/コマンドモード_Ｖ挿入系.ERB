IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%を抱きかかえ、ペニスを突き入れる
	;下の部位専有判定と別にするため9を返す
	RETURN 9
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = 正面から%CALLNAME:プレイヤー番号%に抱きかかえられ、ペニスを咥え込んだ
	;下の部位専有判定と別にするため9を返す
	RETURN 9
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;挿入で使用するのはペニスと膣で1を返す
SELECTCASE 部位番号
	CASE 0
		SIF 処理モード == "プレイヤー"
			RETURN 1
	CASE 2
		;抱えるので手は使用中
		SIF 処理モード == "プレイヤー"
			RETURN 1
	CASE 5
		SIF 処理モード == "ターゲット"
			RETURN 1
		SIF 処理モード == "プレイヤー"
			RETURN 2
	CASE 6
		;尻は届かない
		RETURN 2
ENDSELECT

