			実行値 -= 5
			PRINT 潤滑不足
			PRINT (5)
			＋表示フラグ = 1
		ENDIF
		;ABL:A感覚
		IF ABL:Ａ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ａ感覚 * 2
			PRINT Ａ感覚
			PRINTFORM LV{ABL:Ａ感覚}({ABL:Ａ感覚 * 2})
			＋表示フラグ = 1
		ENDIF
		;A経験
		IF EXP:Ａ経験 < EXPLV:2
			PRINT  - 
			実行値 -= 15
			PRINT Ａ経験
			PRINT 不足
			PRINT (15)
			＋表示フラグ = 1
		ENDIF
	CASE 6
		;ニプルキャップ
		;ABL:Ｂ感覚
		IF ABL:Ｂ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ｂ感覚 * 2
			PRINT Ｂ感覚
			PRINTFORM LV{ABL:Ｂ感覚}({ABL:Ｂ感覚 * 2})
			＋表示フラグ = 1
		ENDIF
		;B経験
		IF EXP:Ｂ経験 < EXPLV:2
			PRINT  - 
			実行値 -= 15
			PRINT Ｂ経験
			PRINT 不足
			PRINT (15)
			＋表示フラグ = 1
		ENDIF
ENDSELECT

