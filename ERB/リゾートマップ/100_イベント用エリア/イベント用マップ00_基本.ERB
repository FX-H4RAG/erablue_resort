@DRAW_MAP_BG_100_FESTIVAL_お花見(SLICED)
#DIMS SLICED
SELECTCASE SLICED
	CASE "CFLAG:MASTER:現在位置"
		SETCOLOR 0x90EE90
		PRINTS SLICED
		RESETCOLOR
	CASE "桜"
		SETCOLOR 0xFFDBED
		PRINTS SLICED
		RESETCOLOR
	CASE "池"
		SETCOLOR 0x0067C0
		PRINTS SLICED
		RESETCOLOR
	CASEELSE
		PRINTS SLICED
ENDSELECT









