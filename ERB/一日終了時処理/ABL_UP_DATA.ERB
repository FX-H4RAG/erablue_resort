	CASE 8
		LOCAL:0 = 60000
	CASE 9
		LOCAL:0 = 70000
	CASEELSE
		LOCAL:0 = 20000 * (ABL:(ARG:2):Ｃ感覚 - 6)
ENDSELECT
;経験補正
LOCAL:0 = LOCAL:0 * 10 / (5 + GETEXPLV(EXP:(ARG:2):Ｃ経験,8))
