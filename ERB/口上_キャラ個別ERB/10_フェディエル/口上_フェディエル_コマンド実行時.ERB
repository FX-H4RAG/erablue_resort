;-------------------------------------------------
;COM380_0_305 渓流エリアで釣りをする
;-------------------------------------------------
;RFLAG:コマンド結果受渡し変数
	;ミニゲームの種類  1:簡易釣りモード  2:ちゃんと釣りモード
;RFLAG:コマンド結果受渡し変数２
	;どのくらい釣れたか、魚の数が直接入る  10以上で大漁、5以上で上々、0でボウズ

@KOJO_COM_10_380_0_305(対象キャラ)
#DIM 対象キャラ
CALL 口上変数初期化
SELECTCASE RFLAG:コマンド結果受渡し変数２
	CASE IS >= 10
		;大漁
		SELECTCASE RAND:3
			CASE 0
				KSTR:(K++) = 「ほほ～、こうも釣れると中々爽快よの！」
			CASE 1
				KSTR:(K++) = 「魚、魚、魚…今から『ばーべきゅー』が楽しみで仕方ないぞよ！じゅるり…」
			CASEELSE
				KSTR:(K++) = 「これだけあるなら一匹二匹%CSTR:対象キャラ:一人称%が貰って行っても…いっ？！…%CSTR:対象キャラ:二人称%はけちんぼじゃな！」
				KSTR:(K++) = 勝手に魚を持って行こうとした%CALLNAME:対象キャラ%の手の甲を軽く叩いて止めた…。
		ENDSELECT
	CASE IS >= 5
		;上々
		SELECTCASE RAND:3
			CASE 0
				KSTR:(K++) = 「結構釣れたようじゃの！成果があると案外楽しいのう」
			CASE 1
				KSTR:(K++) = 「ほほっ、中々いきが良い魚共じゃな！」
			CASEELSE
				KSTR:(K++) = 「これは如何様に調理すると美味かや？」
		ENDSELECT
	CASE IS > 0
		;普通
		SELECTCASE RAND:3
			CASE 0
				KSTR:(K++) = 「直接獲りに行く方が良いのではないかや？」
			CASE 1
				KSTR:(K++) = 「…時間をかけてたったこれだけかえ？釣りとは中々難儀なものじゃの…」
			CASEELSE
				KSTR:(K++) = 「むむ、これでは%CSTR:対象キャラ:一人称%の腹は満たされぬぞ…」
				KSTR:(K++) = この竜、しれっと全部自分が食べるつもりでいる…。
		ENDSELECT
	CASEELSE
		;釣果ゼロ
		SELECTCASE RAND:3
			CASE 0
				KSTR:(K++) = 「まさかとは思うが…何も居ない水面に糸を垂らしているのではあるまいな？」
			CASE 1
				KSTR:(K++) = 「ただ悪戯に時間を浪費するのみ、か…不毛じゃの…」
			CASEELSE
				KSTR:(K++) = 「ふぁ………」
				KSTR:(K++) = あまりにも釣果が上がらなかったからか、%CALLNAME:対象キャラ%は釣りに飽きてしまっている…。
		ENDSELECT
ENDSELECT

CALL 口上変数表示(対象キャラ, 0)

