;取得快楽：Ｂ

CALL 口上変数初期化
LOCAL = FINDCHARA(NO, 10)

IF PLAYER == MASTER
	;相手があなたの時
	IF TALENT:LOCAL:恋慕
		SELECTCASE CUP:LOCAL:快Ｂ
			;各部位の累積が10000で絶頂
			CASE IS > 10000
				;即絶頂
				KSTR:(K++) = 
			CASE IS > 6000
				;かなり感じている
				KSTR:(K++) = 
			CASE IS > 3000
				;人並みに感じている
				KSTR:(K++) = 
			CASEELSE
				;まだ快楽に慣れていない
				KSTR:(K++) = 
		ENDSELECT
	ELSEIF TALENT:LOCAL:セフレ
		KSTR:(K++) = 
	ELSE
		KSTR:(K++) = 
	ENDIF
ENDIF

CALL 口上変数表示(LOCAL, 0)

