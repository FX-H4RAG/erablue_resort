		KSTR:(K++) = 「足でしてほしい？　うん、良いよ～っ、じゃあうつ伏せになって……」
		KSTR:(K++) = 「背中じゃなくて、おちんちん？　…………そ、そっかぁ」
		KSTR:(K++) = 「……痛くしたら、ダメなんだよね？」
	ELSEIF NOWEX:PLAYER:射精
		KSTR:(K++) = 「足でびくびく感じるの、変な感じ～」
		KSTR:(K++) = 「でも……ちょっと楽しい、かも……_H_」
	ELSE
		SELECTCASE RAND:3
			CASE 2
				KSTR:(K++) = 「チチリちゃんの足で、もうイッちゃいそう？」
				KSTR:(K++) = 「いいよ～足でふみふみされて～、よわよわおちんちんイッちゃえ_H_　ざぁ～こっ_H_」
			CASE 1
				KSTR:(K++) = 「マッサージの時に、変なクセ出ちゃったらどうしようかな～っ」
				KSTR:(K++) = 「あ、でも……おちんちん扱いてる動きなの、へんたいお師さん以外にはわからないか～っ_H_」
			CASE 0
				KSTR:(K++) = 「き、気持ちいいの……？　ほんとに？　そ、そっかアタシ、もっと頑張るね？」
		ENDSELECT
	ENDIF
ENDIF

