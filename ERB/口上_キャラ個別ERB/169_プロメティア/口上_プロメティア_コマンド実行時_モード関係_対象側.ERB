			CASE IS > 3000
				;人並みに感じている
				KSTR:(K++) = 
			CASEELSE
				;まだ快楽に慣れていない
				KSTR:(K++) = 
		ENDSELECT
	ELSEIF TALENT:対象キャラ:セフレ
		KSTR:(K++) = 
	ELSE
		KSTR:(K++) = 
	ENDIF
ENDIF

CALL 口上変数表示(対象キャラ, 0)

