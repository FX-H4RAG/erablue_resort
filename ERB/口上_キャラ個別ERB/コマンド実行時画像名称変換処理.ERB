;
@コマンド実行時画像名称変換処理(対象キャラ, コマンド名)
#FUNCTIONS
#DIM  対象キャラ
#DIMS コマンド名
#DIMS コマンド画像名称

;DEBUGPRINTFORML @コマンド実行時画像名称変換処理(%CALLNAME:対象キャラ%, %コマンド名%)

コマンド画像名称 '= ""
コマンド画像名称 '= GRAP_COM_RAND(対象キャラ, "コマンド実行時_", CSTR:対象キャラ:表示グラフィックカテゴリ, コマンド名)

;DEBUGPRINTFORML  コマンド画像名称 == %コマンド画像名称%

SIF コマンド画像名称 != ""
	RETURNF コマンド画像名称


SELECTCASE 1
	CASE GROUPMATCH(コマンド名, "Ｖ挿入する", "Ｇスポ責め", "ポルチオ責め", "種付けプレス", "子宮口挿入", "角ハンドル", "尻尾ひっぱり後背位", "首筋甘噛み交尾", "おっぱい手綱正常位") > 0
		;V性交系
		コマンド画像名称 '= グラ表示用文字列算出(対象キャラ, "コマンド実行時_")
	CASE GROUPMATCH(コマンド名, "入口を責める", "子宮裏を責める", "Ｓ字結腸責め", "角ハンドルＡ", "尻尾ひっぱり後背位A") > 0
		;A性交系
		コマンド画像名称 '= グラ表示用文字列算出(対象キャラ, "コマンド実行時_")
	CASEELSE
ENDSELECT

コマンド画像名称 '= REPLACE(コマンド画像名称, "顔_", "")
コマンド画像名称 '= REPLACE(コマンド画像名称, "短冊_", "")
コマンド画像名称 '= REPLACE(コマンド画像名称, "差分_", "")
コマンド画像名称 '= REPLACE(コマンド画像名称, @"%CSTR:対象キャラ:表示グラフィックカテゴリ%_", "")

;DEBUGPRINTFORML  コマンド画像名称 == %コマンド画像名称%

IF コマンド画像名称 != ""
	コマンド画像名称 '= GRAP_COM_RAND(対象キャラ, "コマンド実行時_", CSTR:対象キャラ:表示グラフィックカテゴリ, @"%コマンド名%%コマンド画像名称%")

	;DEBUGPRINTFORML  コマンド画像名称 == %コマンド画像名称%

	SIF コマンド画像名称 == ""
		コマンド画像名称 '= GRAP_COM_RAND(対象キャラ, "コマンド実行時_", CSTR:対象キャラ:表示グラフィックカテゴリ, コマンド画像名称)
ENDIF

;DEBUGPRINTFORML  コマンド画像名称 == %コマンド画像名称%

RETURNF コマンド画像名称

