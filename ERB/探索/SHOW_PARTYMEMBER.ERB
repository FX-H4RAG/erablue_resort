	ENDIF
	LOCALS += @"<font color='#%属性数値文字色変換_HTML(表示用ステータス格納配列:LOCAL:9)%'>"
	LOCALS += @"%属性数値文字列変換(表示用ステータス格納配列:LOCAL:9), 4%</font>"
	LOCALS += @"{表示用ステータス格納配列:LOCAL:1, 6}"
