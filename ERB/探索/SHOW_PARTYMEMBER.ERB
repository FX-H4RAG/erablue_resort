	CASE 9010
		;隊列を詰める
		FOR 番号管理, 入れ替え候補_メンバー, メインサブ * 10 + 4
			BATTLE_STATE:番号管理:0 = BATTLE_STATE:(番号管理 + 1):0
		NEXT
		入れ替え候補_メンバー = -1
		入れ替え候補_リスト = -1
	CASE 0 TO 13
		IF 入れ替え候補_リスト > 0
			BATTLE_STATE:LOCAL:0 = 入れ替え候補_リスト
			;メイン/サブの方をチェック
			FOR 番号管理, !メインサブ * 10, !メインサブ * 10 + 4
				IF BATTLE_STATE:番号管理:0 == 入れ替え候補_リスト
					FOR LOCAL, 番号管理, !メインサブ * 10 + 4
						BATTLE_STATE:LOCAL:0 = BATTLE_STATE:(LOCAL + 1):0
					NEXT
					BREAK
				ENDIF
			NEXT
			入れ替え候補_メンバー = -1
			入れ替え候補_リスト = -1
			ページ番号 = 0
		ELSE
			IF 入れ替え候補_メンバー == LOCAL
				入れ替え候補_メンバー = -1
			ELSE
				入れ替え候補_メンバー = LOCAL
			ENDIF
		ENDIF
	CASE 50
		ソート法則 = 属性
		入れ替え候補_リスト = -1
	CASE 51
		ソート法則 = 
		入れ替え候補_リスト = -1
	CASE 52
		ソート法則 = 固有
		入れ替え候補_リスト = -1
	CASE 9999
		入れ替え候補_メンバー = -1
		入れ替え候補_リスト = -1
		ページ番号 = 0
		RETURN -1
	CASE IS >= 100
		IF 入れ替え候補_メンバー > -1
			BATTLE_STATE:入れ替え候補_メンバー:0 = 表示用ステータス格納配列:(LOCAL - 100):0
			;メイン/サブの方をチェック
			FOR 番号管理, !メインサブ * 10, !メインサブ * 10 + 4
				IF BATTLE_STATE:番号管理:0 == BATTLE_STATE:入れ替え候補_メンバー:0
					FOR LOCAL, 番号管理, !メインサブ * 10 + 4
						BATTLE_STATE:LOCAL:0 = BATTLE_STATE:(LOCAL + 1):0
					NEXT
					BREAK
				ENDIF
			NEXT
			入れ替え候補_メンバー = -1
			入れ替え候補_リスト = -1
			ページ番号 = 0
		ELSE
			IF 入れ替え候補_リスト == 表示用ステータス格納配列:(LOCAL - 100):0
				入れ替え候補_リスト = -1
			ELSE
				入れ替え候補_リスト = 表示用ステータス格納配列:(LOCAL - 100):0
			ENDIF
		ENDIF
ENDSELECT

