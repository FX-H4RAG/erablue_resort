
@マップチップリソース名変換(ARGS)
#FUNCTIONS

SELECTCASE ARGS
	CASE "木"
		RETURNF "壁_森"
	CASE "原"
		RETURNF "床_草原"
	CASE "空"
		RETURNF "青空"
	CASE "雲"
		RETURNF "青空_雲"
ENDSELECT





@イベントアイコン数字リソース名変換(ARG)
#FUNCTIONS

SELECTCASE ARG
	CASE 1
		RETURNF "エネミーアイコン"
	CASE 2
		RETURNF "会話アイコン"
	CASE 3
		RETURNF "宝箱アイコン_木箱"
	CASE 4
		RETURNF "宝箱アイコン_木箱_開"
	CASE 5
		RETURNF "出口アイコン"
	CASE 6
		RETURNF "会話アイコン_赤"
	CASE 99
		RETURNF "透過アイコン"
ENDSELECT

