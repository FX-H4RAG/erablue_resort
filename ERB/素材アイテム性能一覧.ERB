
@素材アイテム登録処理
#DIMS 比較用一時保存配列, 1000
#DIMS 素材名分割用, 2
VARSET 比較用一時保存配列

FOR LOCAL, 0, DT_ROW_LENGTH("所持素材データベース")
	比較用一時保存配列:LOCAL = %DT_CELL_GETS("所持素材データベース", LOCAL, "素材アイテム名")%
NEXT

ENUMFUNCBEGINSWITH "素材性能記述_"
FOR COUNT, 0, RESULT
	SPLIT RESULTS:COUNT, "_", 素材名分割用
	IF !MATCH(比較用一時保存配列, 素材名分割用:1)
		CALLFORM %RESULTS:COUNT%
		DT_ROW_ADD "所持素材データベース", "素材アイテム名", 素材名分割用:1, "所持素材数", 0, "累計入手素材数", 0, "素材カテゴリ", 素材性能_カテゴリ, "素材ランク", 素材性能_素材ランク, "素材属性", 属性文字列数値変換(素材性能_属性), "素材値相場", 100
	ENDIF
NEXT


