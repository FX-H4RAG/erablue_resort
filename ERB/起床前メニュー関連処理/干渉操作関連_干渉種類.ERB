@干渉強化関数_1(ARGS, キャラ番号)
#DIM キャラ番号

TSTR:コマンド名受渡 = 
SELECTCASE ARGS
	CASE "名前"
		TSTR:コマンド名受渡 = 干渉・友情増幅
	CASE "詳細文章"
		TSTR:コマンド名受渡 = 好感度要素「友情」の上昇量を増加させる干渉。<br>
		TSTR:コマンド名受渡 += "eraの契約者であるあなたに対し、友誼の感情を抱きやすくなる。<br>"
		TSTR:コマンド名受渡 += "以下のうち、大きい方の効果が適用される。<br>"
		TSTR:コマンド名受渡 += "・好感度要素「友情」の増加量 ＋［干渉力のランク✕２０］％<br>"
		TSTR:コマンド名受渡 += "・好感度要素「友情」の増加量 ＋［（干渉力のランク＋２）✕１０］<br>"
		TSTR:コマンド名受渡 += "<br>"
		TSTR:コマンド名受渡 += "経験値取得タイミング：友情度上昇時<br>"
	CASE "値段"
		RETURN 1
	CASE "適用条件"
		SIF 干渉強化変数:キャラ番号:干渉種類 != 1
			RETURN 0
		RETURN NOWEX:キャラ番号:友情度上昇格納
	CASE "実補正処理"
		IF NOWEX:キャラ番号:友情度上昇格納
			LOCAL = MAX(NOWEX:キャラ番号:友情度上昇格納 * (干渉強化変数:キャラ番号:干渉ランク) / 5, (干渉強化変数:キャラ番号:干渉ランク + 2) * 10)
			NOWEX:キャラ番号:友情度上昇格納 += LOCAL
			干渉強化変数:キャラ番号:干渉経験値 += 1
		ENDIF
	CASE "対象値"
		TSTR:コマンド名受渡 = 友情度：{MIN(CFLAG:キャラ番号:友情度,100000),6}
ENDSELECT

